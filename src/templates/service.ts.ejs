import { Injectable } from '@nestjs/common';
import { InjectRepository } from '@nestjs/typeorm';
import { Repository } from 'typeorm';
import <%= entityClassName %> from './entities/<%= fileNameCut %>.entity';
import { Create<%= entityClassName %>Dto } from './dtos/<%= fileNameCut %>-create.dto';
import { Update<%= entityClassName %>Dto } from './dtos/<%= fileNameCut %>-update.dto';

@Injectable()
export class <%= entityClassName %>Service {
  constructor(
    @InjectRepository(<%= entityClassName %>)
    private <%= entityName %>Repository: Repository<<%= entityClassName %>>,
  ) {}

  findAll() {
    return this.<%= entityName %>Repository.find();
  }

  findOne(id: number) {
    return this.<%= entityName %>Repository.findOne({
      where: {
        id,
      },
    });
  }

  create(create<%= entityClassName %>Dto: Create<%= entityClassName %>Dto) {
    return this.<%= entityName %>Repository.save(create<%= entityClassName %>Dto);
  }

  update(id: number, update<%= entityClassName %>Dto: Update<%= entityClassName %>Dto) {
    return this.<%= entityName %>Repository.update(id, update<%= entityClassName %>Dto);
  }

  remove(id: string) {
    return this.<%= entityName %>Repository.delete(id);
  }
}


{
  "name": "@rharkor/nest-crud",
  "version": "1.0.7",
  "description": "NestJS CRUD generator",
  "main": "index.js",
  "bin": {
    "nest-crud": "./dist/index.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "copy-templates": "cp -r ./src/templates ./dist/src",
    "build": "rm -rf dist && tsc && npm run copy-templates"
  },
  "dependencies": {
    "@nestjs/cli": "^8.0.0",
    "@rharkor/utils": "^0.1.2",
    "ejs": "^3.1.9"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.6",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "nestjs",
    "cli",
    "crud",
    "generator"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "author": "HUORT Louis",
  "license": "ISC"
}
